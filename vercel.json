{
    "version": 2,
    "builds": [
      {
        "src": "dist/event-connect-front/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "functions": {
      "api/**/*.ts": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "static": {
      "maxAge": "1d",
      "immutable": true
    }
  }
  